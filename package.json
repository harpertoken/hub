{
  "name": "hub",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "@google/generative-ai": "^0.2.1",
    "@monaco-editor/react": "^4.7.0",
    "@radix-ui/react-tabs": "^1.1.4",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.6.2",
    "cors": "^2.8.5",
    "dompurify": "^3.2.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "firebase": "^10.7.1",
    "form-data": "^4.0.0",
    "framer-motion": "^12.6.3",
    "katex": "^0.16.22",
    "lowlight": "^3.3.0",
    "lucide-react": "^0.294.0",
    "marked": "^9.1.5",
    "mermaid": "^11.6.0",
    "multer": "^2.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "react-hot-toast": "^2.5.2",
    "react-markdown": "^9.0.1",
    "react-router-dom": "^6.30.0",
    "react-scripts": "^5.0.1",
    "react-syntax-highlighter": "^15.6.1",
    "react-toastify": "^9.1.3",
    "rehype-katex": "^7.0.1",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "uuid": "^11.1.0",
    "web-vitals": "^2.1.4",
    "xmldom": "^0.6.0"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "DISABLE_ESLINT_PLUGIN=true GENERATE_SOURCEMAP=false react-scripts build",
    "test": "react-scripts test",
    "lint": "eslint src --ext .js,.jsx",
    "check": "npm run lint && npm run build && npm run test -- --watchAll=false --passWithNoTests --coverage",
    "eject": "react-scripts eject",
    "server": "node src/server/index.js",
    "dev-server": "nodemon src/server/index.js",
    "kill-server": "node kill-server.js",
    "dev": "concurrently --names \"client,server\" --prefix \"[{name}]\" \"npm start --silent\" \"npm run dev-server --silent\"",
    "dev-with-env": "cross-env REACT_APP_GEMINI_API_KEY=your_key concurrently \"npm start\" \"npm run dev-server\"",
    "update-changelog": "node copy-changelog.js",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:debug": "playwright test --debug",
    "e2e:smoke": "playwright test --grep \"@smoke\" --workers=2",
    "e2e:accessibility": "playwright test --grep \"@accessibility\" --workers=2",
    "e2e:performance": "playwright test --grep \"@performance\" --workers=1",
    "e2e:fast": "playwright test --workers=4",
    "e2e:ci": "playwright test --workers=3 --reporter=github",
    "e2e:report": "playwright show-report"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!react-markdown|remark|rehype|vfile|unist|bail|is-plain-obj|trough|zwitch|property-information|hast-util|web-namespaces|space-separated-tokens|comma-separated-tokens|escape-string-regexp|trim-lines|collapse-white-space|trim-trailing-lines|devlop|mdast-util|micromark|parse-entities|character-entities|decode-named-character-reference|ccount|longest-streak|mdast-util-math|katex|hastscript|parse5|unist-util-visit-parents|mdast-util-find-and-replace|mdast-util-gfm-autolink-literal|mdast-util-gfm|markdown-table|html-void-elements)"
    ],
    "moduleNameMapper": {
      "^remark-math$": "<rootDir>/src/__mocks__/remark-math.js",
      "^rehype-katex$": "<rootDir>/src/__mocks__/rehype-katex.js",
      "^react-syntax-highlighter$": "<rootDir>/src/__mocks__/react-syntax-highlighter.js",
      "^react-syntax-highlighter/dist/esm/styles/prism$": "<rootDir>/src/__mocks__/prism-styles.js",
      "^unist-util-visit-parents$": "<rootDir>/src/__mocks__/unist-util-visit-parents.js",
      "^remark-gfm$": "<rootDir>/src/__mocks__/remark-gfm.js",
      "^rehype-raw$": "<rootDir>/src/__mocks__/rehype-raw.js"
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "express-rate-limit": "^7.1.5",
    "node-fetch": "^2.7.0",
    "nodemon": "^3.0.1",
    "playwright": "^1.40.0",
    "sharp": "^0.34.2",
    "tailwindcss": "^3.3.5"
  },
  "packageManager": "pnpm@10.11.0+sha512.6540583f41cc5f628eb3d9773ecee802f4f9ef9923cc45b69890fb47991d4b092964694ec3a4f738a420c918a333062c8b925d312f42e4f0c263eb603551f977",

  "engines": {
    "node": ">=18.0.0"
  }
}
