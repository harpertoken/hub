{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build",
        "buildCommand": "pnpm run build",
        "installCommand": "pnpm install"
      }
    },
    {
      "src": "src/server/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/education-query",
      "dest": "/src/server/index.js"
    },
    {
      "src": "/education-query",
      "dest": "/src/server/index.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/src/server/index.js"
    },
    {
      "src": "/test",
      "dest": "/src/server/index.js"
    },
    {
      "src": "/test-gemini",
      "dest": "/src/server/index.js"
    },
    {
      "src": "/process-video",
      "dest": "/src/server/index.js"
    },
    {
      "src": "/process-video-url",
      "dest": "/src/server/index.js"
    },
    {
      "src": "/process-audio",
      "dest": "/src/server/index.js"
    },
    {
      "src": "/process-youtube",
      "dest": "/src/server/index.js"
    },
    {
      "src": "/analyze-github-user",
      "dest": "/src/server/index.js"
    },
    {
      "src": "/analyze-github-repo",
      "dest": "/src/server/index.js"
    },
    {
      "src": "/edi-prompt",
      "dest": "/src/server/index.js"
    },
    {
      "src": "/gemini-text",
      "dest": "/src/server/index.js"
    },
    {
      "src": "/suggest-prompts",
      "dest": "/src/server/index.js"
    },
    {
      "src": "/process-image",
      "dest": "/src/server/index.js"
    },
    {
      "src": "/create-shareable-link",
      "dest": "/src/server/index.js"
    },
    {
      "src": "/shared-video/(.*)",
      "dest": "/src/server/index.js"
    },
    {
      "src": "/shared-video-file/(.*)",
      "dest": "/src/server/index.js"
    },
    {
      "src": "/convert-video",
      "dest": "/src/server/index.js"
    },

    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "regions": ["iad1"],
  "framework": "create-react-app",
  "env": {
    "NODE_ENV": "production"
  }
}
